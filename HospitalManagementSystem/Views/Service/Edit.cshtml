

@model HospitalManagementSystem.Models.Service
@{
    ViewBag.Title = "Edit";
}

<h2>Edit</h2>


<form action="/Service/Update/@Model.ServiceId" method="POST">
    <div>
        <label>ServiceId:</label>
        <input type="number" name="ServiceId" value="@Model.ServiceId" />
    </div>

    <div>
        <label>Service Name:</label>
        <input type="text" name="ServiceName" value="@Model.ServiceName" />
    </div>

    <div>
        <input type="submit" name="Update Service" />
    </div>

</form>